dy,\r\nh1,\r\nh2,\r\nh3,\r\nh4,\r\np,\r\nfigure,\r\nblockquote,\r\ndl,\r\ndd {\r\n  margin: 0;\r\n}\r\n\r\n/* Remove list styles on ul, ol elements with a list role, which suggests default styling will be removed */\r\nul[role='list'],\r\nol[role='list'] {\r\n  list-style: none;\r\n}\r\n\r\n/* Set core root defaults */\r\nhtml:focus-within {\r\n  scroll-behavior: smooth;\r\n}\r\n\r\n/* Set core body defaults */\r\nbody {\r\n  min-height: 100vh;\r\n  text-rendering: optimizeSpeed;\r\n  line-height: 1.5;\r\n}\r\n\r\n/* A elements that don't have a class get default styles */\r\na:not([class]) {\r\n  text-decoration-skip-ink: auto;\r\n}\r\n\r\n/* Make images easier to work with */\r\nimg,\r\npicture {\r\n  max-width: 100%;\r\n  display: block;\r\n}\r\n\r\n/* Inherit fonts for inputs and buttons */\r\ninput,\r\nbutton,\r\ntextarea,\r\nselect {\r\n  font: inherit;\r\n}\r\n\r\n/* Remove all animations, transitions and smooth scroll for people that prefer not to see them */\r\n@media (prefers-reduced-motion: reduce) {\r\n  html:focus-within {\r\n   scroll-behavior: auto;\r\n  }\r\n  \r\n  *,\r\n  *::before,\r\n  *::after {\r\n    animation-duration: 0.01ms !important;\r\n    animation-iteration-count: 1 !important;\r\n    transition-duration: 0.01ms !important;\r\n    scroll-behavior: auto !important;\r\n  }\r\n}","scss\\base\\_base.scss":"body {\r\n  background: var(--clr-neutral-100);\r\n  line-height: 1.5;\r\n  font-family: system-ui, sans-serif;\r\n}\r\n\r\nh1,\r\nh2,\r\nh3 {\r\n  font-weight: bold;\r\n}\r\n","scss\\components\\_fancy-cards.scss":".fancy-card-grid {\r\n  // just for the demo\r\n\r\n  margin: 6rem auto;\r\n  width: min(100% - 2rem, 70rem);\r\n\r\n  display: grid;\r\n  gap: 1rem;\r\n  grid-template-columns: repeat(4, 1fr);\r\n}\r\n\r\n.fancy-card {\r\n  position: relative;\r\n  display: grid;\r\n  background-color: var(--clr-neutral-100);\r\n  text-decoration: none;\r\n  color: inherit;\r\n  padding: 1rem;\r\n  border: 1px solid var(--clr-neutral-400);\r\n  border-bottom: 5px solid var(--clr-primary);\r\n  aspect-ratio: 1 / 0.7;\r\n  grid-row: 2;\r\n  z-index: 1;\r\n  transition: transform 350ms ease, background-color 350ms ease,\r\n    color 350ms ease, z-index 0ms -350ms linear;\r\n\r\n  &__number {\r\n    color: var(--clr-primary);\r\n    width: min-content;\r\n    margin-left: auto;\r\n  }\r\n\r\n  &__arrow {\r\n    font-size: 2rem;\r\n    color: var(--clr-primary);\r\n    position: absolute;\r\n    inset: auto 1rem 0.25rem auto;\r\n    opacity: 0;\r\n    transform: translateX(-50%);\r\n    transition: transform 200ms ease-in, opacity 200ms linear;\r\n  }\r\n\r\n  &__image {\r\n    position: absolute;\r\n    z-index: -2;\r\n    bottom: 0;\r\n    max-width: 70%;\r\n    max-height: 70%;\r\n  }\r\n\r\n  &__title {\r\n    font-size: 2.25rem;\r\n    font-weight: 600;\r\n    line-height: 1;\r\n    width: min-content;\r\n    margin-top: auto;\r\n  }\r\n\r\n  &--large {\r\n    aspect-ratio: 1 / 1;\r\n  }\r\n\r\n  &--offset-up {\r\n    grid-row: 1 / span 2;\r\n  }\r\n\r\n  &--offset-down {\r\n    grid-row: 2 / span 2;\r\n  }\r\n\r\n  &::before,\r\n  &::after {\r\n    content: \"\";\r\n    position: absolute;\r\n    inset: 0;\r\n  }\r\n\r\n  &::before {\r\n    pointer-events: none;\r\n    opacity: 0;\r\n    content: \"\";\r\n    position: absolute;\r\n    inset: 0;\r\n    background: black;\r\n    filter: blur(2em);\r\n    z-index: -3;\r\n    transform: translate(-50%, 50%);\r\n    transition: opacity 350ms ease, transform 350ms ease;\r\n  }\r\n\r\n  &::after {\r\n    z-index: -1;\r\n    background: var(--clr-neutral-100);\r\n    transition: background-color 350ms ease;\r\n  }\r\n\r\n  &:hover,\r\n  &:focus {\r\n    background: var(--clr-neutral-900);\r\n    transform: scale(1.35);\r\n    border: 0;\r\n    z-index: 10;\r\n    transition: transform 350ms ease, color 350ms ease;\r\n    animation: shake 500ms linear;\r\n    animation-delay: 500ms;\r\n\r\n    .fancy-card__title {\r\n      color: var(--clr-neutral-100);\r\n    }\r\n\r\n    .fancy-card__arrow {\r\n      opacity: 1;\r\n      transform: translateX(0);\r\n    }\r\n\r\n    .fancy-card__image {\r\n      transition: transform 150ms ease;\r\n    }\r\n\r\n    @for $image from 0 through 4 {\r\n      .fancy-card__image:nth-of-type(#{$image + 1 }) {\r\n        transition-delay: 500ms + ($image * 75);\r\n      }\r\n    }\r\n\r\n    .fancy-card__image:nth-of-type(1) {\r\n      transform: translate(150%, -10%) rotate(10deg);\r\n    }\r\n    .fancy-card__image:nth-of-type(2) {\r\n      transform: translate(120%, 80%) rotate(-10deg);\r\n    }\r\n    .fancy-card__image:nth-of-type(3) {\r\n      transform: translate(0%, 90%) rotate(5deg);\r\n    }\r\n    .fancy-card__image:nth-of-type(4) {\r\n      transform: translate(-120%, 50%) rotate(-15deg);\r\n    }\r\n    .fancy-card__image:nth-of-type(5) {\r\n      transform: translate(-90%, -50%) rotate(5deg);\r\n    }\r\n\r\n    &::before {\r\n      opacity: 0.4;\r\n      transform: translate(-50%, 50%);\r\n    }\r\n\r\n    &::after {\r\n      background-color: var(--clr-neutral-900);\r\n    }\r\n  }\r\n}\r\n\r\n@keyframes shake {\r\n  0% {\r\n    transform: scale(1.35);\r\n  }\r\n  25% {\r\n    transform: scale(1.35) rotate(5deg);\r\n  }\r\n  40% {\r\n    transform: scale(1.35) rotate(-5deg);\r\n  }\r\n  50% {\r\n    transform: scale(1.35) rotate(2deg);\r\n  }\r\n  60% {\r\n    transform: scale(1.35) rotate(-5deg);\r\n  }\r\n  55% {\r\n    transform: scale(1.35) rotate(-2deg);\r\n  }\r\n  100% {\r\n    transform: scale(1.35);\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"fefaf7d288d52527c5631a9a831c0092","cacheData":{"env":{}}}